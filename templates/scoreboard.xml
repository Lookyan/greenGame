<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<div class="module"> 
		<div class="score">
			<table class="module__score_table">
				<fest:each iterate="json" index="i" value="top">
					<tr>
						<td><fest:value>top.name</fest:value></td>
						<td><fest:value>top.score</fest:value></td>
					</tr>
				</fest:each>	
			</table>

			<a href="/#" class="login__js-back"> Back </a>
		</div>
	</div>

</fest:template>